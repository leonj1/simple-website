# LocalStack Pro Configuration
# Copy this file to .env and fill in your values

# Required: LocalStack Pro license token
LOCALSTACK_AUTH_TOKEN=your-localstack-pro-license-token-here

# Optional: Debug mode (0 or 1)
DEBUG=0

# Optional: Enable persistence (0 or 1)
PERSISTENCE=0

# Optional: AWS services to enable (comma-separated)
SERVICES=s3,lambda,dynamodb,sqs,sns,cloudformation,iam,ssm,secretsmanager

# Optional: AWS region
AWS_DEFAULT_REGION=us-east-1

# Optional: Lambda executor type
LAMBDA_EXECUTOR=docker

# Optional: CORS configuration
DISABLE_CORS_CHECKS=0
EXTRA_CORS_ALLOWED_ORIGINS=

# Optional: External hostname
HOSTNAME_EXTERNAL=localhost